{
  "AzureAd": {
    "Instance": "https://login.microsoftonline.com/",
    "TenantId": "<tenantId of backend AppReg>",
    "ClientId": "<clientId of backend AppReg>"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AppFunctions": {
    "DismissUserRisk": "<authcontext id for DismissUserRisk>",
    "GenerateTap": "<authcontext id for GenerateTap>",
    "ResetPassword": "<authcontext id for ResetPassword>"
  },
  "Tap": {
    "LifetimeInMinutes": 60,
    "IsUsableOnce": true
  },
  "Frontend": {
    "frontendClientId": "<frontendClientId>",
    "tenantId": "<tenantId>",
    "backendClientId": "<backendClientId>",
    "customCssUrl": "<optional public url to custom css>",
    "appTitle": "<optional appTitle>",
    "faviconUrl": "<optional faviconUrl>"
  },
  "VerifiedId": {
    "DecentralizedIdentifier": "<didId Secret of your tenant>",
    "BackendUrl": "<BackendUrl>",
    "JwtSigningKey": "<SomeRandomSECRETString - In Production recommended to use keyvault>",
    "TargetSecurityAttribute": "<target security attribute where the verified claim should be written to>",
    "TargetSecurityAttributeSet": "<target security attribute set where the verified claim should be written to>",
    "CreatePresentationRequestUri": "https://verifiedid.did.msidentity.com/v1.0/verifiableCredentials/createPresentationRequest",
    "FaceMatchConfidenceThreshold": 70
  },
  "LocalDevSettings": { // Needs only to be filled out when running locally
    "VerifiedIdClientId": "<clientId of AppReg with VerifiableCredential.Create.All permissions>",
    "VerifiedIdTenantId": "<tenantId of AppReg with VerifiableCredential.Create.All permissions>",
    "VerifiedIdSecret": "<secret of AppReg with VerifiableCredential.Create.All permissions>"
  },
  "AllowedHosts": "*"
}